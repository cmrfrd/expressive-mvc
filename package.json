    "@types/react": "^16.9.46",
    "jest": "^26.4.2",
    "react": ">=16.8.0",
    "react-test-renderer": "^16.13.1",
    "rollup": "^2.26.5",
    "rollup-plugin-typescript2": "^0.27.2",
