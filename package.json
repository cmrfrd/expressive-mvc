{
  "name": "react-use-controller",
  "version": "1.0.0-beta.35",
  "description": "Easy to use state controllers using classes and hooks",
  "author": {
    "name": "Gabe Klein",
    "email": "gabe@termtm.com",
    "url": "https://github.com/gabeklein"
  },
  "license": "MIT",
  "repository": "https://github.com/gabeklein/expressive-mvc",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "hooks"
  ],
  "main": "lib/index.js",
  "module": "src/index.ts",
  "types": "index.d.ts",
  "files": [
    "lib/",
    "index.d.ts"
  ],
  "scripts": {
    "build": "rollup --config",
    "watch": "rollup --config --watch --sourcemap",
    "start": "npm run --prefix examples start",
    "prebuild": "rm -rf lib/",
    "publish:beta": "npm version prerelease --preid=beta",
    "preversion": "jest",
    "postversion": "npm publish",
    "prepublish": "npm run build",
    "postpublish": "git push; git push --tags; rm -rf lib",
    "test": "jest --watchAll"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@testing-library/react-hooks": "^3.4.1",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.46",
    "jest": "^26.4.2",
    "react": ">=16.8.0",
    "react-test-renderer": "^16.13.1",
    "rollup": "^2.26.5",
    "rollup-plugin-typescript2": "^0.27.2",
    "ts-jest": "^26.2.0",
    "tslib": "^2.0.1",
    "typescript": "typescript@>=3.8 <4.0"
  }
}
